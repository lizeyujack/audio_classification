   "source": [
    "import tarfile\n",
    "import os\n",
    " \n",
    "#创建压缩包名\n",
    "tar = tarfile.open(\"./development.tar.gz\",\"w:gz\")\n",
    "#创建压缩包\n",
    "for root,dir,files in os.walk(\"development\"):\n",
    "    for file in files:\n",
    "        fullpath = os.path.join(root,file)\n",
    "        tar.add(fullpath)\n",
    "tar.close()"
   ]
